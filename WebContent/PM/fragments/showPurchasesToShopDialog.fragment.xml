<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form">
	<Dialog id="purchasesFromShop" title="Einkäufe">


		<List xmlns="sap.m" items="{shopPurchases>/}">
				<items>
					<StandardListItem xmlns="sap.m"
						visible="true"
						info="{shopPurchases>eink_wert} €"
						description=""
						title="{path: 'shopPurchases>eink_datum',
								formatter: '.formatter.overviewPurchaseDateFormatter'
								}"
						infoState="None">
					</StandardListItem>
				</items>
				<swipeContent></swipeContent> <!-- sap.ui.core.Control -->
				<headerToolbar></headerToolbar> <!-- sap.m.Toolbar, since 1.16 -->
				<infoToolbar></infoToolbar> <!-- sap.m.Toolbar, since 1.16 -->
				<columns> <!-- sap.m.Column -->
				</columns>
		</List>



		<buttons> <!-- sap.m.Button, since 1.21.1 -->
			<Button xmlns="sap.m" text="zurück" press="onClosePurchasesDialog">
			</Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>