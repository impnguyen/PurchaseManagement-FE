						xmlns="sap.m" type="Active" visible="true" title="{menu>title}" description="" icon="" iconInset="true" iconDensityAware="true" activeIcon="" info="" infoState="None" adaptTitleSize="true" titleTextDirection="Inherit" infoTextDirection="Inherit" ariaLabelledBy="" validateFieldGroup="" tap="" detailTap="" press="onMenuPress" detailPress="">
