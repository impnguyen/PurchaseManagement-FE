<core:View 
	xmlns:l="sap.ui.layout" 
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:u="sap.ui.unified" controllerName="mpn.PM.controller.menu" 
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page 
		xmlns="sap.m" id="menuListPage" busy="false" busyIndicatorDelay="1000" visible="true" titleLevel="Auto" showNavButton="false" showHeader="true" showSubHeader="false" navButtonText="" navButtonTooltip="" enableScrolling="true" icon="" backgroundDesign="Standard" navButtonType="Back" showFooter="true" contentOnlyBusy="false" floatingFooter="false">
		<content>
			<List 
				xmlns="sap.m" id="menuList" busy="false" busyIndicatorDelay="10" visible="true" inset="false" headerText="" headerDesign="Standard" footerText="" mode="SingleSelect" width="100%" showUnread="false" noDataText="" showNoData="true" enableBusyIndicator="true" modeAnimationOn="true" showSeparators="All" swipeDirection="Both" growing="false" growingThreshold="20" growingTriggerText="" growingScrollToLoad="false" growingDirection="Downwards" rememberSelections="true" keyboardMode="Navigation" backgroundDesign="Solid" items="{menu>/menuItems}">
				<items>
					<!-- sap.m.ListItemBase -->
