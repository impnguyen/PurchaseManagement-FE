		</content>
		<!-- sap.ui.core.Control -->
		<customHeader>
			<Bar>
				<contentLeft>
					<button icon="http://icons.iconarchive.com/icons/tinylab/android-lollipop-apps/128/DrawIt-icon.png"></button>
				</contentLeft>
				<contentMiddle>
					<Label text="{i18n>menuPageTitle}"/>
				</contentMiddle>
			</Bar>
		</customHeader>
		<!-- sap.m.IBar -->
		<footer></footer>
		<!-- sap.m.IBar -->
		<subHeader></subHeader>
		<!-- sap.m.IBar -->
		<headerContent></headerContent>
		<!-- sap.ui.core.Control -->
		<landmarkInfo></landmarkInfo>
		<!-- sap.m.PageAccessibleLandmarkInfo -->
	</Page>
