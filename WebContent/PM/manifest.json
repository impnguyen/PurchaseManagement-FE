{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "PM",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "Purchase Management",
		"description": "A purchase managment tool",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"dataSources": {
			"menu_de": {
				"uri": "model/menu_de.json",
				"type": "JSON"
			},
			"menu_en": {
				"uri": "model/menu_en.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "mpn.PM.views.app",
			"type": "XML"
		},
		"contentDensities": {
			"cozy": true,
			"compact": true
		},
		"dependencies": {
			"minUI5Version": "1.50",
			"libs": {
				"sap.m": {

				},
				"sap.ui.layout": {

				},
				"sap.ui.unified": {

				}
			}
		},
		"resources": {
			"css": [{
				"uri": "styles/style.css",
				"id": "purchaseStyle"
			}]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"menu_de": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "menu_de"
			},
			"menu_en": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "menu_en"
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "mpn.PM.views",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": "true",
				"clearTarget": "true",
				"transition": "flip"
			},
			"routes": [{
					"pattern": "",
					"name": "dashboard",
					"target": "dashboard"
				},
				{
					"pattern": "myGroup/{groupId}",
					"name": "myGroup",
					"target": ["myGroup"]
				}
			],
			"targets": {
				"dashboard": {
					"viewName": "dashboard",
					"viewLevel": 2
				},
				"myGroup": {
					"viewName": "split",
					"viewLevel": 1
				}
			}
		}
	}
}